<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.UiKitDemo"
        tools:targetApi="31">
        <activity
            android:name="com.tech.android.base.uikitdemo.update.UpdateActivity"
            android:exported="false" />
        <activity
            android:name="com.tech.android.base.uikitdemo.camera.CoreCameraActivity"
            android:exported="false" />
        <activity android:name="com.tech.android.base.uikitdemo.recyclerview.PageGridInViewPagerActivity" />
        <activity android:name="com.tech.android.base.uikitdemo.recyclerview.PageGridLayoutActivity" />
        <activity android:name="com.tech.android.base.uikitdemo.recyclerview.GlideActivity" />
        <activity android:name="com.tech.android.base.uikitdemo.recyclerview.ViewPagerActivity" />
        <activity android:name="com.tech.android.base.uikitdemo.recyclerview.ViewPager2Activity" />
        <activity android:name="com.tech.android.base.uikitdemo.recyclerview.List1Activity" />
        <activity android:name="com.tech.android.base.uikitdemo.recyclerview.ListDemoActivity" />
        <activity android:name="com.tech.android.base.uikitdemo.recyclerview.RecyclerViewDemoActivity" />
        <activity
            android:name="com.tech.android.base.uikitdemo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tech.android.base.uikitdemo.BannerDemoActivity" />
    </application>

</manifest>